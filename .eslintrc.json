{
    "extends": "standard",
    "rules": {
        "indent": ["error", 4],
        "semi": ["error", "always"],
        "space-before-function-paren": "off",
        "no-multi-spaces": "off",
        "one-var": "off",
        "no-useless-constructor": "off"
    },
    "env": {
        "browser": true
    }
}